// Example IDL file for our monster's schema.
namespace tutorial;
enum PhoneType:byte { MOBILE = 0, HOME, WORK = 2 }

table PhoneNumber {
  number:string;
  type:PhoneType;
}

table Person {
  name:string;
  id:short;
  email:string;

  phones:[PhoneNumber];        // Vector of Tables.
}
root_type Person;
